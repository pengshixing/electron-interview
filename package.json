{
  "name": "electron-interview",
  "version": "0.0.1",
  "author": "pengshixing",
  "description": "简历平台桌面应用。",
  "main": "./dist/electron.js",
  "scripts": {
    "start:main": "webpack --config ./webpack/webpack.main.dev.js && electron ./dist/electron.js",
    "start:render": "webpack-dev-server --config ./webpack/webpack.render.dev.js",
    "css": "tailwindcss -i ./app/renderer/index.css -o ./app/renderer/app.css --watch",
    "dev": "concurrently \"wait-on -t 3s && npm run start:main\" \"npm run start:render\"",
    "build:main": "webpack --config ./webpack/webpack.main.prod.js",
    "build:render": "webpack --config ./webpack/webpack.render.dev.js",
    "build-win": "electron-builder --win --x64",
    "build-linux": "electron-builder --linux",
    "build-mac": "electron-builder --mac",
    "build": "rm -rf dist package && npm run build:main && npm run build:render"
  },
  "build": {
    "appId": "electronInterview.ElectronReact",
    "productName": "electronInterview",
    "copyright": "Copyright © 2024 ${author}",
    "extends": null,
    "files": [
      "dist/**/*",
      "package.json",
      "node_modules/"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "package"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "dist/assets/logo-512x512.png"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ],
      "icon": "dist/assets/logo-256x256.png"
    },
    "linux": {
      "target": [
        "deb",
        "rpm"
      ],
      "category": "Utility",
      "icon": "dist/assets/logo-256x256.png"
    }
  },
  "dependencies": {
    "@emotion/styled": "^11.13.0",
    "autoprefixer": "^10.4.19",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "react": "^18.3.0",
    "react-css-modules": "^4.7.11",
    "react-dom": "^18.3.0",
    "react-router": "^6.23.0",
    "react-router-dom": "^6.23.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-syntax-jsx": "^7.24.1",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.24.1",
    "@dr.pogodin/babel-plugin-react-css-modules": "^6.13.1",
    "@types/lodash": "^4.17.13",
    "@types/react": "^18.3.0",
    "@types/react-css-modules": "^4.6.8",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.1",
    "electron": "^30.0.1",
    "electron-builder": "^25.1.8",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.4.5",
    "wait-on": "^7.2.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  }
}
